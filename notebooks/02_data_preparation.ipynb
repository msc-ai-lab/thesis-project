    "def check_distribution(df: pd.DataFrame, dataset_name: str) -> None:\n",
    "  \"\"\"\n",
    "  Check and visualise the distribution of malignancy labels and skin tones in a dataset\n",
    "  \n",
    "  Parameters:\n",
    "  -----------\n",
    "  df : pandas.DataFrame\n",
    "    The dataset to check\n",
    "  dataset_name : str\n",
    "    Name of the dataset for display purposes\n",
    "  \"\"\"\n",
    "  # Calculate overall statistics\n",
    "  total_samples = len(df)\n",
    "  \n",
    "  # Print label distribution\n",
    "  print(f\"\\n{dataset_name} Dataset Statistics:\")\n",
    "  print(f\"Total samples: {total_samples}\")\n",
    "  \n",
    "  # Plot skin tone distribution by class\n",
    "  benign_skin_dist = df[df['malignant'] == 0]['skin_tone'].value_counts().sort_index()\n",
    "  malignant_skin_dist = df[df['malignant'] == 1]['skin_tone'].value_counts().sort_index()\n",
    "  \n",
    "  # Create a combined DataFrame for grouped bar chart\n",
    "  all_tones = sorted(set(benign_skin_dist.index) | set(malignant_skin_dist.index))\n",
    "  benign_counts = [benign_skin_dist.get(tone, 0) for tone in all_tones]\n",
    "  malignant_counts = [malignant_skin_dist.get(tone, 0) for tone in all_tones]\n",
    "  \n",
    "  return all_tones, benign_counts, malignant_counts, total_samples\n",
    "\n",
    "# Get data for all datasets\n",
    "full_data = check_distribution(metadata, \"Full\")\n",
    "train_data = check_distribution(train_df, \"Training\")\n",
    "val_data = check_distribution(val_df, \"Validation\")\n",
    "test_data = check_distribution(test_df, \"Testing\")\n",
    "\n",
    "# Create a figure with subplots\n",
    "fig, axs = plt.subplots(1, 3, figsize=(18, 5))\n",
    "\n",
    "# Plot each dataset\n",
    "datasets = [train_data, val_data, test_data]\n",
    "titles = [\"Training\", \"Validation\", \"Testing\"]\n",
    "\n",
    "# Prepare data for plotting\n",
    "for i, (data, title) in enumerate(zip(datasets, titles)):\n",
    "    all_tones, benign_counts, malignant_counts, total = data\n",
    "    x = np.arange(len(all_tones))\n",
    "    width = 0.35\n",
    "    \n",
    "    axs[i].bar(x - width/2, benign_counts, width, label='Benign', color='skyblue')\n",
    "    axs[i].bar(x + width/2, malignant_counts, width, label='Malignant', color='salmon')\n",
    "    axs[i].set_title(f'{title} Dataset: Skin Tone by Class (n={total})')\n",
    "    axs[i].set_xlabel('Skin Tone')\n",
    "    axs[i].set_ylabel('Count')\n",
    "    axs[i].set_xticks(x)\n",
    "    axs[i].set_xticklabels(all_tones)\n",
    "    \n",
    "    # Only show legend on first plot to avoid redundancy\n",
    "    if i == 0:\n",
    "        axs[i].legend()\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
