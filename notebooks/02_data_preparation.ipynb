    "ham_data = data_path / 'raw_dataset' / 'HAM10000'\n",
    "ham_df = pd.read_csv(ham_data / 'metadata.csv')\n",
    "ham_images = ham_data / 'images'\n",
    "\n",
    "# Check if all images in ham_df exist in the folder\n",
    "image_files = [f.stem for f in ham_images.glob('*.jpg')]\n",
    "\n",
    "# Filter the DataFrame to keep only rows where image_id exists as a file\n",
    "original_len = len(ham_df)\n",
    "ham_df = ham_df[ham_df['image_id'].isin(image_files)]\n",
    "print(f\"Removed {original_len - len(ham_df)} rows where image files don't exist in {ham_images}\")\n",
    "\n",
    "# Rename image_id column to DDI_file to match other dataframes\n",
    "ham_df = ham_df.rename(columns={'image_id': 'DDI_file'})\n",
    "\n",
    "# Add path information to DDI_file\n",
    "ham_df['DDI_file'] = 'raw_dataset/HAM10000/images/' + ham_df['DDI_file'] + '.jpg'\n",
    "ham_df['malignant'] = ham_df['dx'].apply(lambda x: 1 if x in ['mel', 'bcc', 'akiec'] else 0)\n",
    "ham_df.head()"
   ]
